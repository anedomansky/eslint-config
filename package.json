{
  "name": "@anedomansky/eslint-config",
  "version": "2.0.0",
  "description": "Opinionated ESLint configuration for quality code",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "lint": "eslint ./src",
    "build": "npm run clean && tsc",
    "clean": "tsc --build --clean",
    "prepublishOnly": "npm run lint && npm run build",
    "eslint:config": "eslint --print-config ./test/test.html"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/anedomansky/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript-eslint",
    "typescript",
    "prettier",
    "formatter",
    "code style"
  ],
  "author": "anedomansky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anedomansky/eslint-config/issues"
  },
  "homepage": "https://github.com/anedomansky/eslint-config#readme",
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": "*",
    "typescript-eslint": "^8.39.1"
  },
  "devDependencies": {
    "eslint": "9.33.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0"
  },
  "dependencies": {
    "@eslint/js": "9.33.0",
    "@eslint/json": "0.13.1",
    "@html-eslint/eslint-plugin": "0.45.0",
    "@html-eslint/parser": "0.45.0",
    "@ngrx/eslint-plugin": "20.0.0",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "@typescript-eslint/utils": "8.40.0",
    "angular-eslint": "20.2.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-compat": "6.0.2",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-playwright": "2.2.2",
    "eslint-plugin-regexp": "2.10.0",
    "eslint-plugin-testing-library": "7.6.6",
    "eslint-plugin-unused-imports": "4.2.0",
    "prettier": "3.6.2"
  }
}
