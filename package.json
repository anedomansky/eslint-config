{
  "name": "@anedomansky/eslint-config",
  "version": "1.2.0",
  "description": "Opinionated ESLint configuration for quality code",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "tsc --build --clean",
    "prepublishOnly": "npm run build",
    "eslint:config": "eslint --print-config ./test/test.json"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/anedomansky/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript-eslint",
    "typescript",
    "prettier",
    "formatter",
    "code style"
  ],
  "author": "Alexander Nedomansky <anedomansky@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/anedomansky/eslint-config/issues"
  },
  "homepage": "https://gitlab.com/anedomansky/eslint-config#readme",
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": "*",
    "typescript-eslint": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "9.31.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.38.0"
  },
  "dependencies": {
    "@eslint/js": "9.31.0",
    "@eslint/json": "0.13.1",
    "@html-eslint/eslint-plugin": "0.43.1",
    "@html-eslint/parser": "0.43.0",
    "@ngrx/eslint-plugin": "19.2.1",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "@typescript-eslint/utils": "8.38.0",
    "angular-eslint": "20.1.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-compat": "6.0.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-playwright": "2.2.0",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-testing-library": "7.6.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "prettier": "3.6.2"
  }
}
